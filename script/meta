#$EPIC$
/*
	Meta script... binds <F1> to <F5>

	function characters do ^[[1x~
	where x is the function key
*/

bind ^[[11~ type f1
bind ^[[12~ type f2
bind ^[[13~ type f3
bind ^[[14~ type f4
bind ^[[15~ type f5

bind ^[OP switch_channels
bind ^[OQ clear_screen
bind ^[OR type msg santabot
bind ^[OS parse_command leave *
